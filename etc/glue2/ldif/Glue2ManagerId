
#
# GLUE2ManagerId
#
dn: GLUE2ManagerID=GLUE2ManagerId,GLUE2ServiceId=$GLUE2ServiceId,GLUE2GroupID=resource,o=glue
objectClass: GLUE2Manager
objectClass: GLUE2StorageManager
GLUE2ManagerId: $GLUE2ManagerId
GLUE2ManagerProductName: storm-backend-server
GLUE2ManagerProductVersion: `rpm -q --queryformat='%{VERSION}' storm-backend-server`
GLUE2ManagerServiceForeignKey: $GLUE2ManagerServiceForeignKey
GLUE2StorageManagerStorageServiceForeignKey: $GLUE2StorageManagerStorageServiceForeignKey